  "version": "0.2.0",
  "configurations": [
    {
      "type": "lldb",
      "request": "launch",
      "name": "Debug executable 'http_fs'",
      "cargo": {
        "args": [
          "build",
          "--bin=http_fs",
          "--package=http_fs"
        ],
        "filter": {
          "name": "http_fs",
          "kind": "bin"
        }
      },
      "args": [
        // "-d",
        "--mount-point",
        "Z:",
        "-u",
        "http://localhost:5223",
        "-j",
        "tests\\tree.json",
        "-i"
      ],
      "cwd": "${workspaceFolder}",
      "env": {
        "RUST_LOG": "debug",
        "TERM": "xterm-256color",
      },
      "console": "externalTerminal"
    },
    {
      "type": "lldb",
      "request": "launch",
      "name": "Debug unit tests in executable 'http_fs'",
      "cargo": {
        "args": [
          "test",
          "--no-run",
          "--bin=http_fs",
          "--package=http_fs"
        ],
        "filter": {
          "name": "http_fs",
          "kind": "bin"
        }
      },
      "args": [],
      "cwd": "${workspaceFolder}",
      "env": {
        "RUST_LOG": "info"
      }
    }
  ]
}